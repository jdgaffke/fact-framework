<?xml version="1.0" encoding="UTF-8"?>
<operator 
    name="entityName"
    created="20141001"
    deprecated=""
    retired=""
    implementation="entityName.20141001.js"
    >

    <note> 
    Returns a single field containing the entity common name.
    For organizations, this is the org name.
    For contacts, this is a concatenation of first, middle init, last.
    </note>        
    
    <example> 
        <query v="20141001">
            <entitySearch all="your search terms">
                <entityName/>
            </entitySearch>
        </query>
    </example>        
    
    <input> 
        <field name="entityId" type="EntityId">
    </input>        
        
    <output>
        <field name="entityName" type="String" />
    </output>        
    
</operator>

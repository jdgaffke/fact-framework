<?xml version="1.0" encoding="UTF-8" ?>
<macro name="client.hierarchy" created="20141001" deprecated="" retired="">

    <note>
        Returns account hierarchy for a contact.
    </note>

    <example note="Account hierarchy for a known contact.">
        <query v="20141001">
            <client.hierarchy c:contactId="1234" />
        </query>
    </example>

    <expansion>
        <contact.account v="20141001">
            <account />
        </contact.account>
    </expansion>

</macro>
